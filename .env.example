# Praxis Configuration
# Copy this file to .env and customize as needed

# Ollama Configuration
OLLAMA_HOST=localhost
OLLAMA_PORT=11434

# Model Configuration
# Orchestrator: model for function calling / tool selection
PRAXIS_ORCHESTRATOR_MODEL=functiongemma

# Executor: model for code generation and responses
PRAXIS_EXECUTOR_MODEL=gemma3:4b

# Browser Configuration
PRAXIS_BROWSER_ENABLED=true
PRAXIS_BROWSER_SESSION=praxis
PRAXIS_BROWSER_HEADED=false

# Debug Mode
PRAXIS_DEBUG=false
